{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/ams": "^3",
    "@sap/cds": "^9",
    "@sap/hana-client": "^2",
    "@sap/xssec": "^4",
    "express": "^4",
    "sqlite3": "^5"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^2",
    "@sap/ams-dev": "^2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "sapux": [
    "app/admin-books",
    "app/browse"
  ],
  "cds": {
    "requires": {
      "auth": {
        "kind": "dummy"
      }
    },
    "build": {
      "tasks": [
        {
          "for": "ams",
          "src": "srv",
          "options": {
            "model": ["srv", "app"]
          }
        }
      ]
    }
  }
}
